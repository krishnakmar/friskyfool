    <style>
        /* Container for the images */
        .carousel-container {
            overflow: hidden;
            white-space: nowrap;
            width: 100%;
            display: flex;
            align-items: center;
        }

        /* Scrolling images */
        .carousel-track {
            display: inline-flex;
            animation: scroll 10s linear infinite;
        }

        /* Styling for each image */
        .carousel-track img {
            width: 9%;
            border-radius: 50%;
            margin: 0 20px;
        }

        /* Keyframes for scrolling animation */
        @keyframes scroll {
            0% {
                transform: translateX(0);
            }

            100% {
                transform: translateX(-100%);
            }
        }

        /* Pausing animation on hover */
        .carousel-track:hover {
            animation-play-state: paused;
        }

        .phone-icon {
            position: fixed;
            bottom: 20px;
            right: 50px;
            width: 120px;
            height: 50px;
            background-color: #f9dc5c;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);
            display: flex;
            align-items: center;
            justify-content: center;
            z-index: 1000;
            text-decoration: none;
            border-radius: 8px;
            color: #000 !important;
        }

        .phone-icon img {
            width: 60%;
            height: auto;
        }

        .carousel-container {
            width: 100%;
            overflow: hidden;
            margin: 20px auto;
            position: relative;
        }

        .oursupportheading h2 {
            padding: 0;
            margin: 0;
            font-size: 100px;
            text-transform: uppercase;
            color: #f9dc5c;
            padding-bottom: 40px;
            text-align: center;
            font-family: 'HumaneRegular', sans-serif;
            font-weight: 400;
            margin-top: 60px;
        }

        .carousel-track {
            display: flex;
            transition: transform 0.5s ease-in-out;
        }



        .carousel-btn {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            background-color: rgba(0, 0, 0, 0.5);
            color: white;
            border: none;
            padding: 10px;
            cursor: pointer;
            font-size: 24px;
            z-index: 1;
        }

        .carousel-track a {
            display: contents !important;
        }


        .carousel-btn.left {
            left: 10px;
        }

        .carousel-btn.right {
            right: 10px;
        }

        /* Responsive styles */
        @media (max-width: 600px) {
            .carousel-track img {
                width: 50%;
                /* Display 2 images in a row on smaller screens */
            }
        }


        body {
            overflow-x: hidden;

